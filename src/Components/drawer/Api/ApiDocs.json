      "Contest": {
        "type": "object",
        "properties": {
          "type": {
            "type": "enum",
            "enum": ["RATED", "UNRATED"],
            "example": "RATED"
          },
          "_id": {
            "type": "string",
            "example": "5b5c89ef30db8a0020962414"
          },
          "code": {
            "type": "string",
            "example": "UNIVERSE"
          },
          "name": {
            "type": "string",
            "example": "Universe"
          },
          "description": {
            "type": "string",
            "example": "Contest open for 5 year with playlists problems"
          },
          "startsAt": {
            "type": "string",
            "example": "2018-07-28T03:30:00.000Z"
          },
          "endsAt": {
            "type": "string",
            "example": "2023-07-28T03:30:00.000Z"
          },
          "contestAdmin": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "_id": {
                  "type": "string",
                  "example": "5b6d84eb2e9049002068df33"
                },
                "name": {
                  "type": "string",
                  "example": "Barath Suresh Nadar"
