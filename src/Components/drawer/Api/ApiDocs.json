  "paths": {
    "/contests": {
      "get": {
        "tags": ["Contest"],
        "summary": "Returns information about all available contests",
        "description": "Returns information about all available contests",
        "parameters": [
          {
            "name": "Accept",
            "in": "header",
            "description": "Accept header",
            "required": true,
            "schema": {
              "type": "string",
              "example": "application/vnd.arena+json;version=1"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/vnd.arena+json;version=1": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Contest"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Invalid status value"
          },
          "500": {
            "description": "Some error. Try again"
          }
        }
      }
    },
